<h2>Registered Vets</h2>

<%= form_for(:session, url: login_path) do |f| %> 
    <%= f.label "Email:" %>
    <%= f.text_field :email %>
    <br>
    <%= f.label "Password:" %>
    <%= f.password_field :password %>
    <br><br>

    <%= f.submit "Log In" %>

    <p>Not registered? <%= link_to "Register now!", new_vet_url %></p>
<% end %>
